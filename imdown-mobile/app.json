{
  "expo": {
    "name": "ImDown",
    "slug": "imdown",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imdown.imdown",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.1097983281822-8qr8vltrud1hj3rfme2khn1lmbj2s522"
        }
      }
    }
  }
}
